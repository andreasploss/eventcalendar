<div id='event-table' class="table-responsive">
    <table class="table table-striped">
      <thead>
        <tr>
            <td>ID</td>
            <td>Datum</td>
            <td>Uhrzeit</td>
            <td>Titel der Veranstaltung</td>
            <td>Location</td>
            <td>Status</td>
            <td>URL</td>
            <td>Bild</td>
            <td>Kategorie Id und Titel</td>
            <td>Reihe</td>
            <td>Highlight</td>
            <td>Topevent</td>
            <td>Wiederholungs-Regel</td>

        </tr>
      </thead>
      <tbody class="event-items">
        {{#each this}}
            <tr class="event-item showcontent" data-tags="{{category.id}}" >
                <td>{{ id }}</td>
                <td>{{formatDate eventdate 'dd. MMM Â´yy'}}</td>
                <td>
                {{#if timeValid}}
                    <span class="event-time">
                        <time>{{formatDate eventdate 'HH:mm'}} Uhr</time>
                    </span>
                {{/if}}
              </td>
              <td>{{title}}</td>
              <td>{{location}}</td>
              <td>{{status}}</td>
              <td>{{url}}</td>
              <td><img src="{{image.thumb_100px}}" width="100" /><br />{{image.thumb_100px}}</td>
              <td>{{category.id}}<br />{{category.title}}</td>
              <td>{{series}}</td>
              <td>{{highlight}}</td>
              <td>{{topevent}}</td>
              <td>{{rule}}</td>

            </tr>
        {{/each}}
      </tbody>
    </table>
</div>